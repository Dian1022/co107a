/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    //  Nand ( in1 = a, in2 = b , out = aNandb);
    Or   (in1 = a , in2 = b, out = aOrb);
    And  (a = aNandb , b = aOrb, out=out);
}
