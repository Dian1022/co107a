/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel , out = notsel) ;
    And(in1 = a , in2 = notsel , out = aAndnotsel) ;
    And(in1 = b , in2 = sel , out = bAndsel ) ;
    OR(in1 = aAndnotsel , in2 = bAndsel) ;
}
