/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    And(in1 = a[0] , in2 = b[0] , out = Andout[0]) ; 
    And(in1 = a[1] , in2 = b[1] , out = Andout[1]) ;
    And(in1 = a[2] , in2 = b[2] , out = Andout[2]) ; 
    And(in1 = a[3] , in2 = b[3] , out = Andout[3]) ; 
    And(in1 = a[4] , in2 = b[4] , out = Andout[4]) ; 
    And(in1 = a[5] , in2 = b[5] , out = Andout[5]) ; 
    And(in1 = a[6] , in2 = b[6] , out = Andout[6]) ; 
    And(in1 = a[7] , in2 = b[7] , out = Andout[7]) ; 
    And(in1 = a[8] , in2 = b[8] , out = Andout[8]) ; 
    And(in1 = a[9] , in2 = b[9] , out = Andout[9]) ; 
    And(in1 = a[10] , in2 = b[10] , out = Andout[10]) ; 
    And(in1 = a[11] , in2 = b[11] , out = Andout[11]) ; 
    And(in1 = a[12] , in2 = b[12] , out = Andout[12]) ; 
    And(in1 = a[13] , in2 = b[13] , out = Andout[13]) ; 
    And(in1 = a[14] , in2 = b[14] , out = Andout[14]) ; 
    And(in1 = a[15] , in2 = b[15] , out = Andout[15]) ;
}
